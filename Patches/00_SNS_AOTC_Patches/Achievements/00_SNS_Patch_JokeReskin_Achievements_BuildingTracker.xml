<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Vanilla Achievements Expanded</li>
	</mods>
	<match Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationFindMod">
			<mods>
				<li>Science Never Stops: Ambition of the Cosmic</li>
			</mods>
			<match Class="PatchOperationSequence">
				<operations>
					
                    <li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_Unobtainer"]/label</xpath>
						<value>
							<label>God #### It, I Guess We're Doing This Again</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_Unobtainer"]/description</xpath>
						<value>
							<description>Construct an Overpowered Forge.</description>
						</value>
					</li>

                    <li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_CosmicManipulator"]/label</xpath>
						<value>
							<label>Let The War Crimes Begin</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_CosmicManipulator"]/description</xpath>
						<value>
							<description>Construct a Universe ############ ####### ### ######.</description>
						</value>
					</li>

                    <li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_AtomicFabricator"]/label</xpath>
						<value>
							<label>Torture == Valuable Resources</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_AtomicFabricator"]/description</xpath>
						<value>
							<description>Construct an Atomic Fabricator.</description>
						</value>
					</li>

                    <li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_CosmicCall"]/label</xpath>
						<value>
							<label>#### You, I'm Cutting [Out] The Middle Man (In Half)</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_CosmicCall"]/description</xpath>
						<value>
							<description>Construct the P.A.I.N. Fabricator.</description>
						</value>
					</li>

                    <li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_PinnacleFabricator"]/label</xpath>
						<value>
							<label>Existence Is Torture</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_PinnacleFabricator"]/description</xpath>
						<value>
							<description>Construct the Torture Device.</description>
						</value>
					</li>

                    <li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_Reactor_GenI"]/label</xpath>
						<value>
							<label>It Gets Worse. Much Worse.</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_Reactor_GenI"]/description</xpath>
						<value>
							<description>Construct an Abyssal Reactor Generation 1</description>
						</value>
					</li>

                    <li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_Reactor_GenII"]/label</xpath>
						<value>
							<label>Now You're Cooking With Innocent Lives!</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_Reactor_GenII"]/description</xpath>
						<value>
							<description>Construct an Abyssal Reactor Generation 2</description>
						</value>
					</li>

                    <li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_Reactor_GenIII"]/label</xpath>
						<value>
							<label>The Fallen Legions Wail Again</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>/Defs/AchievementsExpanded.AchievementDef[defName="SNS_Achievement_BuildThing_Reactor_GenIII"]/description</xpath>
						<value>
							<description>Construct an Abyssal Reactor Generation Omega</description>
						</value>
					</li>
					
				</operations>
			</match>
			</li>
		</operations>
	</match>
</Operation>

</Patch>